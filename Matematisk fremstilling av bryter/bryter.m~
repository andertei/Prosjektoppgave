%Definerer konstanter
e_n=2.7182818284590452353602874;
K=1; %Denner er ukjent inntil videre

R_l=22.6;
L_l=87.3*10^(-3);

R_r=1.4;
L_r=18.5*10^(-3);

R_c=100;
C_c=96*10^(-9);

%Her definerer vi tidsrommet til bryteren:
t=0:0.0001:0.1;

for i=1:length(t)
	u_left(i)=K*e_n^(-(R_l/L_l).*t(i));
end

%plot(t, u_left);
